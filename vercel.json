{
    "rewrites": [
        {
            "source": "/procedimento/(.*)",
            "destination": "/index.html"
        },
        {
            "source": "/procedimentos",
            "destination": "/index.html"
        },
        {
            "source": "/admin",
            "destination": "/index.html"
        },
        {
            "source": "/apartments",
            "destination": "/index.html"
        },
        {
            "source": "/booking",
            "destination": "/index.html"
        },
        {
            "source": "/gallery",
            "destination": "/index.html"
        },
        {
            "source": "/contact",
            "destination": "/index.html"
        },
        {
            "source": "/amenities",
            "destination": "/index.html"
        },
        {
            "source": "/blog",
            "destination": "/index.html"
        },
        {
            "source": "/blog/(.*)",
            "destination": "/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*)\\.(?:js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot)$",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000"
                }
            ]
        }
    ],
    "functions": {
        "src/**/*.{js,ts,jsx,tsx}": {
            "runtime": "nodejs18.x"
        }
    }
}